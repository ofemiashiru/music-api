type Artist {
  id: ID!
  artistName: String
  artistImageUrl: String
  genres: [Genre]
}

type ArtistInput {
  id: ID
  artistName: String
  artistImageUrl: String
  genres: [Genre]
}

type Genre {
  id: ID!
  genreName: String
}

type Query {
  getArtists: [Artist]
  getArtist(id: ID!): Artist
  getGenres: [Genre]
}
